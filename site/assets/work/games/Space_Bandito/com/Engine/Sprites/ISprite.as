package com.Engine.Sprites {		import flash.display.Sprite;	import flash.display.DisplayObject;	import flash.events.IEventDispatcher;	import flash.geom.Rectangle;		public interface ISprite extends IEventDispatcher {				//DISPLAY OBJECT PROPERTIES AND METHODS		function get x():Number;		function set x(value:Number):void;		function get y():Number;		function set y(value:Number):void;		function get width():Number;		function set width(value:Number):void;		function get height():Number;		function set height(value:Number):void;		function get rotation():Number;		function set rotation(value:Number):void;		function get hitArea():Sprite;		function getRect(coordinateSpace:DisplayObject):Rectangle;		function get name():String;		function set name(value:String):void;	}}