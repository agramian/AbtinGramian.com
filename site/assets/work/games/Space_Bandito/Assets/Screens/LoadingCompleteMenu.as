package Assets.Screens{		import com.greensock.*;	import com.greensock.easing.*;	import flash.display.Sprite;	import flash.display.SimpleButton;	import flash.events.Event;	import flash.events.MouseEvent;	import com.Engine.Sound.SoundEngine;	import com.Engine.events.APEvent;		public class LoadingCompleteMenu extends Sprite {				public var continueButton:SimpleButton;		protected var se:SoundEngine;				public function LoadingCompleteMenu() {			addEventListener(Event.ADDED_TO_STAGE, addedToStage, false, 0, true);			se = SoundEngine.getInstance();		}				private function addedToStage(e:Event):void {			continueButton.addEventListener(MouseEvent.CLICK, continueButtonClick, false, 0, true);			continueButton.addEventListener(MouseEvent.MOUSE_OVER, continueButtonOver, false, 0, true);		}				private function continueButtonOver(e:MouseEvent):void {			se.playSound("menuOverSound");		}				private function continueButtonClick(e:MouseEvent):void {			se.playSound("menuDownSound");			dispatchEvent(new APEvent(APEvent.HIDE_LOADING));		}			}}